{
  "MyCloudantDB": {
    "searchPatterns": [
      "cloudfoundry:cloudant",
      "env:kube_cloudant_credentials",
      "file:config/my-cloudant-credentials.json"
    ]
  },
  "MyMongoDB": {
    "searchPatterns": [
      "cloudfoundry:mongodb",
      "env:kube_mongodb_credentials",
      "file:config/my-mongo-credentials.json"
    ]
  }
}
